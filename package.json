{
    "name": "quick-saipos",
    "version": "3.0.0",
    "description": "",
    "container-main": "app.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "ndm": "nodemon",
        "debug": "node --inspect-brk=0.0.0.0:9229 ./node_modules/serverless/bin/serverless offline --httpPort 3000",
        "sls": "serverless offline --httpPort 3000",
        "build": "webpack --config webpack.config.mjs",
        "dev": "npm run build && npm run sls",
        "preprod": "npm run build",
        "prod": "git add . && git commit -m",
        "postprod": "git push --all",
        "deploy": "npm ci && export SLS_DEBUG=* && sls deploy -s"
    },
    "nodemonConfig": {
        "watch": [
            "./backend"
        ],
        "ext": "js",
        "exec": "npm run sls"
    },
    "type": "module",
    "author": "mmarcolin",
    "license": "ISC",
    "dependencies": {
        "busboy": "^1.6.0",
        "csv-parser": "^3.0.0",
        "dotenv": "^16.4.5",
        "iconv-lite": "^0.6.3",
        "serverless": "^3.38.0"
    },
    "devDependencies": {
        "@babel/core": "^7.24.5",
        "@babel/preset-env": "^7.24.5",
        "babel-loader": "^9.1.3",
        "css-loader": "^7.1.1",
        "css-minimizer-webpack-plugin": "^6.0.0",
        "mini-css-extract-plugin": "^2.9.0",
        "nodemon": "^3.1.0",
        "serverless-dotenv-plugin": "^6.0.0",
        "serverless-offline": "^13.5.0",
        "terser-webpack-plugin": "^5.3.10",
        "webpack": "^5.91.0",
        "webpack-cli": "^5.1.4"
